{
  "name": "astro-web-components",
  "version": "0.0.1",
  "description": "Astro Web Components",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/astro-web-components/astro-web-components.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook -p 6006 -c .storybook -s static",
    "build-storybook": "build-storybook",
    "astro": "npm-run-all --parallel start storybook"
  },
  "dependencies": {
    "@stencil/core": "^2.0.1"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@stencil/sass": "^1.4.1",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/html": "^6.2.9",
    "@storybook/web-components": "^6.2.9",
    "@types/jest": "^26.0.21",
    "@types/puppeteer": "^5.4.3",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^8.1.1",
    "html-loader": "^2.1.2",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "lit-html": "^1.3.0",
    "markdown-loader": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^5.5.0",
    "storybook-readme": "^5.0.9",
    "write-file-webpack-plugin": "^4.5.1"
  }
}
